{
  "manifest_version": 3,
  "name": "CyberChef Magic Recipe",
  "version": "3.2",

  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "tabs"
  ],

  "action": {
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker":"background.js"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  
}